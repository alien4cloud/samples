              <stringProp name="BeanShellSampler.query">int loopCount = Integer.parseInt(vars.get(&quot;loopCount&quot;));
boolean tearDown = Boolean.parseBoolean(vars.get(&quot;tearDown&quot;));
// we will tear down if tearDown is set to &apos;true&apos; or loopCount greater than 1
vars.put(&quot;doTearDown&quot;, &quot;&quot; + ((loopCount &gt; 1) || tearDown));

log.info(&quot;A4C_BASE: &quot; + vars.get(&quot;a4c_base&quot;));

String prefix = vars.get(&quot;applicationNamePrefix&quot;);
String filePath = vars.get(&quot;resultFolder&quot;) + File.separator + prefix + &quot;-startUp.csv&quot;;
try {
	File file = new File(filePath);
	if(!file.exists()) { 
		BufferedWriter out = new BufferedWriter(new FileWriter(file,true));
		out.write(&quot;ApplicationName;Status;StartDate;Duration_in_sec\n&quot;);
		out.close();
	}	
} catch (IOException e) {
	log.error(&quot;Something went wrong&quot;, e);	
}</stringProp>
